    <color name="mtrl_textinput_default_box_stroke_color" tools:ignore="PrivateResource">#cb3f41</color>
    <color name="grey">#8A8A8A</color>
    <color name="hintTextColor">#BFBFBF</color>
    <color name="red">#ff0000</color>
    <color name="green">#00ff00</color>

    <color name="m3_button_outline_color_selector" tools:ignore="PrivateResource"> #cb3f41
    </color>
    <color name="outlinedEditText">
        <selector  xmlns:android="http://schemas.android.com/apk/res/android">
            <item  android:color="@color/light_grey" android:state_focused="false"/>
            <item  android:color="@color/soft_red_700" android:state_focused="true"/>
        </selector>
    </color>

    <color name="buttonStateChangeColor">
        <selector  xmlns:android="http://schemas.android.com/apk/res/android">
            <item android:color="@color/light_grey" android:state_enabled="false" />
            <item  android:color="@color/soft_red_500" android:state_enabled="true" />

        </selector>
    </color>

    <color name="gridButtonSelectionColor">
        <selector  xmlns:android="http://schemas.android.com/apk/res/android">
        <item android:color="@color/light_grey" android:state_pressed="false" />
        <item  android:color="@color/soft_red_300" android:state_pressed="true" />
        </selector>
    </color>
    <color name="toggleButtonColor">
        <selector  xmlns:android="http://schemas.android.com/apk/res/android">
            <item android:color="@color/soft_red_700" android:state_enabled="false" />
            <item android:color="@color/grey" android:state_enabled="true" />
        </selector>
    </color>


</resources>